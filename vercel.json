{
    "version": 2,
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      },
      {
        "src": "composer.json",
        "use": "@vercel/build-utils",
        "config": {
          "installCommand": "composer install"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ]
  }
  